{
  "name": "inspectify-app",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "start": "node run --debug",
    "test": "node run test --env=jsdom",
    "tslint": "tslint -c tslint.json 'client/**/*.{ts,tsx}'",
    "build": "webpack --config webpack.config.js --mode development",
    "build:prod": "webpack --config webpack.config.prod.js --mode production --process -p"
  },
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "@types/query-string": "^6.2.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "query-string": "^6.2.0",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "4.0.1",
    "redux-localstorage-simple": "^2.1.6",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/classnames": "^2.2.6",
    "@types/history": "4.7.2",
    "@types/jest": "^23.3.9",
    "@types/material-ui": "^0.21.5",
    "@types/react": "16.7.2",
    "@types/react-dom": "16.0.9",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "4.4.1",
    "@types/react-router-dom": "4.3.1",
    "@types/react-router-redux": "5.0.16",
    "@types/webpack": "4.4.18",
    "@types/webpack-env": "1.13.6",
    "aspnet-webpack": "^3.0.0",
    "aspnet-webpack-react": "^4.0.0",
    "autoprefixer": "^9.3.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-minify": "^0.5.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "~3.0.2",
    "html-webpack-plugin": "~3.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "tslint": "^5.13.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "client/**/*.{js,ts,jsx,tsx,mjs}"
    ],
    "mapCoverage": true,
    "testMatch": [
      "<rootDir>/client/**/__tests__/**/*.{js,ts,jsx,tsx,mjs}",
      "<rootDir>/client/**/?(*.)(spec|test).{js,ts,jsx,tsx,mjs}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
